CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE SEQ_LANCAMENTO_ATIVO AS INTEGER START WITH 1 
CREATE MEMORY TABLE LANCAMENTO(DATALANCAMENTO TIMESTAMP NOT NULL PRIMARY KEY,DESCRICAO VARCHAR(128),TIPOLANCAMENTO INTEGER,VALORLANCAMENTO NUMERIC(8,2))
CREATE MEMORY TABLE LANCAMENTO_ATIVO(ID INTEGER NOT NULL PRIMARY KEY,TIPO INTEGER,LANCAMENTO_DATALANCAMENTO TIMESTAMP,CONSTRAINT FK_LANCAMENTO_ATIVO_LANCAMENTO_DATALANCAMENTO FOREIGN KEY(LANCAMENTO_DATALANCAMENTO) REFERENCES LANCAMENTO(DATALANCAMENTO))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
